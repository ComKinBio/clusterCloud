FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
    solver            EulerImplicit;
    method            TDAC;
}

EulerImplicitCoeffs
{
    cTauChem;
}

chemistry       on;

writeFields     true;

reduction
{
    active          off; 
    log             on;
    tolerance       1e-4;
    method          none;
    initialSet
    {
    }
    automaticSIS    off;
    fuelSpecies
    {
    }
}

tabulation
{
    active           off;
    log              on;
    printProportion  off;
    printNumRetrieve off;
    tolerance        1e-3;
    method           ISAT;
    maxNLeafs        5000;
    scaleFactor
    {
        otherSpecies    1;
        Temperature     5000;
        Pressure        1e+5;
        deltaT          1;
    }
}


initialChemicalTimeStep 1e-07;  

#include "WD_multi.OF"

